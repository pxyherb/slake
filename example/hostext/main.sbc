.module main
.access native
.fndecl void print ...

.fn i32 main
	PUSHARG "Entered main\n";

	LOAD %R0, print;
	CALL %R0;
	YIELD 0;

	LVAR i32;
	STORE $0u, 0;
	JMP #loopEnd;
loopStart:
	PUSHARG $0u;
	PUSHARG "\n";
	LOAD %R0, print;
	CALL %R0;
	ADD $0u, $0u, 1;

loopEnd:
	LT %R0, $0u, 100;
	JT #loopStart, %R0;

	RET -1;
.end
