module main;

class Test {
	pub i32 x;

	pub operator new(i32 x) {
		this.x = x;
		return;
	}

	i32 getX() {
		print("Fetched x: ", this.x, "\n");
		return this.x;
	}
}

pub native void print(...);

pub i32 main() {
	print("Entered main\n");

	@Test test = new @Test(100);

	while(test.x = test.x - 1) {
		print("Yielded\n");
		yield test.getX();
	}

	return 114514;
}
